
set(IS_WINDOWS_INSTALLER @WINDOWS_INSTALLER@ )
set(RTDE_BOOST_INCLUDE_DIRS @Boost_INCLUDE_DIRS@ )
set(RTDE_BOOST_LIBRARY_DIR @Boost_LIBRARY_DIR_RELEASE@)

if ("${CMAKE_CXX_STANDARD}" STREQUAL "")
    set(CMAKE_CXX_STANDARD @CMAKE_CXX_STANDARD@)
    set(CMAKE_CXX_STANDARD_REQUIRED @CMAKE_CXX_STANDARD_REQUIRED@)
elseif(${CMAKE_CXX_STANDARD} LESS_EQUAL @CMAKE_CXX_STANDARD@ OR ${CMAKE_CXX_STANDARD} EQUAL 98 )
    set(CMAKE_CXX_STANDARD @CMAKE_CXX_STANDARD@)
    set(CMAKE_CXX_STANDARD_REQUIRED @CMAKE_CXX_STANDARD_REQUIRED@)
endif()